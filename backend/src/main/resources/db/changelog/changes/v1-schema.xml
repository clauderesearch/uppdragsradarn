        </insert>
        <insert tableName="source_types">
            <column name="id" value="77777777-7777-7777-7777-777777777777"/>
            <column name="name" value="EXPERIS"/>
        </insert>
        
        <!-- Status Types -->
        <!-- Assignment Statuses -->
        <insert tableName="status_types">
            <column name="id" value="a1111111-1111-1111-1111-111111111111"/>
            <column name="name" value="ACTIVE"/>
            <column name="entity_type" value="ASSIGNMENT"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="a2222222-2222-2222-2222-222222222222"/>
            <column name="name" value="EXPIRED"/>
            <column name="entity_type" value="ASSIGNMENT"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="a3333333-3333-3333-3333-333333333333"/>
            <column name="name" value="FILLED"/>
            <column name="entity_type" value="ASSIGNMENT"/>
        </insert>
        
        <!-- User Assignment Statuses -->
        <insert tableName="status_types">
            <column name="id" value="b1111111-1111-1111-1111-111111111111"/>
            <column name="name" value="SAVED"/>
            <column name="entity_type" value="USER_ASSIGNMENT"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="b2222222-2222-2222-2222-222222222222"/>
            <column name="name" value="APPLIED"/>
            <column name="entity_type" value="USER_ASSIGNMENT"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="b3333333-3333-3333-3333-333333333333"/>
            <column name="name" value="INTERVIEW"/>
            <column name="entity_type" value="USER_ASSIGNMENT"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="b4444444-4444-4444-4444-444444444444"/>
            <column name="name" value="ACCEPTED"/>
            <column name="entity_type" value="USER_ASSIGNMENT"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="b5555555-5555-5555-5555-555555555555"/>
            <column name="name" value="REJECTED"/>
            <column name="entity_type" value="USER_ASSIGNMENT"/>
        </insert>
        
        <!-- Crawler Job Statuses -->
        <insert tableName="status_types">
            <column name="id" value="c1111111-1111-1111-1111-111111111111"/>
            <column name="name" value="SUCCESS"/>
            <column name="entity_type" value="CRAWLER_JOB"/>
        </insert>
        <insert tableName="status_types">
            <column name="id" value="c2222222-2222-2222-2222-222222222222"/>
            <column name="name" value="FAILED"/>
