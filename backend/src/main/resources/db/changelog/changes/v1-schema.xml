        <insert tableName="currencies">
            <column name="code" value="DKK"/>
            <column name="name" value="Danish Krone"/>
            <column name="symbol" value="kr"/>
        </insert>
        
        <!-- Source Types -->
        <insert tableName="source_types">
            <column name="id" value="11111111-1111-1111-1111-111111111111"/>
            <column name="name" value="EMAIL"/>
        </insert>
        <insert tableName="source_types">
            <column name="id" value="22222222-2222-2222-2222-222222222222"/>
            <column name="name" value="WEB_SCRAPER"/>
        </insert>
        <insert tableName="source_types">
            <column name="id" value="33333333-3333-3333-3333-333333333333"/>
            <column name="name" value="API"/>
        </insert>
        <insert tableName="source_types">
            <column name="id" value="44444444-4444-4444-4444-444444444444"/>
            <column name="name" value="EMAGINE"/>
        </insert>
        <insert tableName="source_types">
            <column name="id" value="55555555-5555-5555-5555-555555555555"/>
            <column name="name" value="ASOCIETYGROUP"/>
        </insert>
        <insert tableName="source_types">
            <column name="id" value="66666666-6666-6666-6666-666666666666"/>
